{
    // Use IntelliSense to learn about possible Node.js debug attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "jsdoc -r test/**/*.js",
            "program": "${workspaceRoot}/node_modules/jsdoc/jsdoc",
            "args": [ "-r", "test/**/*.js" ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Gulp tslint",
            "program": "${workspaceRoot}/node_modules/gulp/bin/gulp.js",
            "args": [
                "tslint"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "preLaunchTask": "default",
            "program": "${workspaceRoot}/lib/bin/tsdoc.js",
            "args": ["./src/**/*.ts"],
            "outFiles": [
                "${workspaceRoot}/lib/**/*.js"
            ]
        }
    ]
}