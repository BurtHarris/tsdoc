{
    "plugins": [
        "plugins/markdown"
    ],
    "markdown": {
        "tags": [
            "foo",
            "bar"
        ]
    },
    "recurseDepth": 10,
    "source": {
        "includePattern": ".(t|j)s(doc|x)?$",
        "excludePattern": ""
    },
    "tags": {
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}